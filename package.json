{
  "name": "felony-auth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "make-user-model": "npx sequelize-cli model:generate --name user --attributes username:string,first_name:string,last_name:string,email:string,password:string",
    "make-two-factor-user-model": "npx sequelize-cli model:generate --name two_factor_user --attributes secret:string,provider:string",
    "migrate": "NODE_ENV=test npx sequelize-cli db:migrate",
    "migrate-env": "DATABASE_URL=postgres://postgres:postgrespw@localhost:5432/felony_auth npx sequelize-cli db:migrate",
    "migrate-down": "npx sequelize-cli db:migrate:undo",
    "migrate-down-all": "npx sequelize-cli db:migrate:undo:all",
    "migrate-down-all-env": "DATABASE_URL=postgres://postgres:postgrespw@localhost:5432/felony_auth npx sequelize-cli db:migrate:undo:all",
    "make-seed-demo-user": "npx sequelize-cli seed:generate --name demo-user",
    "make-seed-demo-two-factor-user": "npx sequelize-cli seed:generate --name demo-two-factor-user",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo",
    "unseed-all": "npx sequelize-cli db:seed:undo:all",
    "test": "jest --detectOpenHandles",
    "jest-clear": "npx jest --clearCache"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/qrcode": "^1.4.2",
    "@types/sequelize": "^4.28.14",
    "@types/uuid": "^8.3.4",
    "bcrypt": "^5.0.1",
    "crypto-ts": "^1.0.2",
    "dotenv": "^16.0.1",
    "mongoose": "^6.4.3",
    "otplib": "^12.0.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.1",
    "redis": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.4",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.0.3",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.19.0",
    "jest": "^28.1.3",
    "sequelize-cli": "^6.4.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
