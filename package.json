{
  "name": "felony-auth",
  "version": "1.0.0",
  "description": "An out-of-the-box authentication module",
  "main": "lib/index",
  "types": "lib/index",
  "author": "Sandro Blavicki",
  "license": "ISC",
  "keywords": [
    "barrage",
    "auth",
    "felony",
    "module"
  ],
  "scripts": {
    "make-user-model": "NODE_ENV=test npx sequelize-cli model:generate --name user --attributes username:string,first_name:string,last_name:string,email:string,password:string",
    "make-two-factor-user-model": "NODE_ENV=test npx sequelize-cli model:generate --name two_factor_user --attributes secret:string,provider:string",
    "migrate": "NODE_ENV=test npx sequelize-cli db:migrate",
    "migrate-down": "NODE_ENV=test npx sequelize-cli db:migrate:undo",
    "migrate-down-all": "NODE_ENV=test npx sequelize-cli db:migrate:undo:all",
    "jest-clear": "npx jest --clearCache",
    "test": "jest --detectOpenHandles --coverage"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "otplib": "^12.0.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.1",
    "redis": "^4.2.0",
    "sequelize": "^6.21.4",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.0.3",
    "@types/qrcode": "^1.5.0",
    "@types/sequelize": "^4.28.14",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.23.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^28.1.3",
    "mockdate": "^3.0.5",
    "sequelize-cli": "^6.4.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}